{
  "name": "dag-hero",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "record:snapshot": "node scripts/record-snapshot.mjs",
    "record:mainnet-60s": "node scripts/record-snapshot.mjs --api-url https://kgi.kaspad.net:3147 --duration-ms 60000 --poll-interval-ms 200 --height-difference 14 --out debug/replay/mainnet-60s.json --compressed-out public/replay/mainnet-60s-compressed.json",
    "perf:check": "npm run build && node scripts/check-perf-budget.mjs"
  },
  "dependencies": {
    "@createjs/tweenjs": "2.0.0-beta.4",
    "@createjs/core": "2.0.0-beta.4",
    "@pixi/graphics-extras": "^6.3.0",
    "pixi.js-legacy": "^6.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.2.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0"
  }
}
